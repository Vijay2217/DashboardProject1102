@model IEnumerable<Dash.Models.ProjectFiles>

@{
    ViewBag.Title = "DisplayProjectList";
}

<h2>DisplayProjectList</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NameP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NameP)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Show Documents", "UploadFiles", new { id=item.Id }) |
            
        </td>
    </tr>
}

</table>
